<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ftfl.icareprofile"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="21" />
    
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    
    <uses-permission android:name="com.android.alarm.permission.SET_ALARM"/>

   
<uses-permission android:name="android.permission.VIBRATE" />


    <application
        
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
                android:theme="@android:style/Theme.Light" >
        <activity
            android:name=".CreateProfileActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".model.Profile"
            android:label="@string/title_activity_profile" >
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name=".ViewProfileActivity"
            android:label="@string/title_activity_view_profile" >
        </activity>
        <activity
            android:name=".CreateDietActivity"
            android:label="@string/title_activity_main" >
        </activity>
        <activity
            android:name=".DietListActivity"
            android:label="@string/title_activity_main" >
            
             
           
        </activity>
        <activity
            android:name=".StartScreenActivity"
            android:label="@string/title_activity_icare_start_screen" >
            
             
        </activity>
        
        <activity
            android:name=".SplashScreenActivity"
            android:label="@string/title_activity_icare_start_screen" >
            
        </activity>
         <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
             
           
        </activity>
        
        <service android:name="com.android.alarmmanagertest.AlarmService" />
        <receiver android:name="com.android.alarmmanagertest.OnAlarmReceiver" />
    </application>

</manifest>
